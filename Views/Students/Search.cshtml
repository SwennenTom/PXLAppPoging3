
@model List<PXLApp.Models.Student>

<h1>Resultaat</h1>
<hr />
@if (Model.Count == 0)
{
    <p>Geen resultaten gevonden</p>
}
else
{
    <div class="card-container">
        @foreach (var item in Model)
        {
            <partial name="studentcard" model="item" />
        }
    </div>
}
<div>
    <a asp-action="Index">Terug naar overzicht</a>
</div>
